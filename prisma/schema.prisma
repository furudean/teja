generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ban {
  uuid          String    @id @db.Uuid
  username      String
  instated      DateTime  @default(now())
  last_modified DateTime  @updatedAt
  revoked       DateTime?
  note          String?
}
